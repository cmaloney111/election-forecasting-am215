# Copy this file to ~/.pypirc and fill in your credentials
# DO NOT commit ~/.pypirc to version control

[distutils]
index-servers =
    pypi
    testpypi

[pypi]
username = __token__
password = <your-pypi-token-here>

[testpypi]
repository = https://test.pypi.org/legacy/
username = __token__
password = <your-testpypi-token-here>
